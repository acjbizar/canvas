{
    "$schema": "https://json.schemastore.org/web-manifest-combined.json",
    "lang": "{{ artwork.lang }}",
    "dir": "{{ artwork.dir }}",
    "name": "{{ artwork.title }}",
    "display": "fullscreen",
    "icons": [{
        "src": "{{ artwork.url }}apple-touch-icon-precomposed.png"
    }],
    "scope": "/",
    "id": "{{ artwork.url }}",
    "theme_color": "{{ artwork.themeColor }}"
}